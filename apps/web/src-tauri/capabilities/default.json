{
  "identifier": "default",
  "description": "Default permissions for Vimix",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:default",
    "shell:allow-execute",
    "shell:allow-spawn",
    "shell:allow-stdin-write",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/Vimix-processor",
          "sidecar": true,
          "args": true
        }
      ]
    },
    "opener:default",
    "updater:default",
    "process:allow-relaunch"
  ]
}
